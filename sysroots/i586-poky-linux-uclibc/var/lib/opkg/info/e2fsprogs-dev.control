Recommends: libcomerr-dev, e2fsprogs-badblocks-dev, libgcc-s-dev, uclibc-backtrace-dev, uclibc-libdl-dev, libe2p-dev, libext2fs-dev, uclibc-libpthread-dev, libuuid-dev, libc-dev, libblkid-dev, libss-dev, util-linux-dev
